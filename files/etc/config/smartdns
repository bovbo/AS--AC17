	option seconddns_no_rule_nameserver '1'
	option seconddns_no_dualstack_selection '1'
	option seconddns_port '5335'
	option seconddns_no_rule_soa '1'
	option dualstack_ip_selection '1'
	option server_name 'OpenWrt88'
	option auto_set_dnsmasq '0'
	option seconddns_no_cache '1'
	option rr_ttl '86400'
	option rr_ttl_min '360'
	option rr_ttl_max '86400'
	option rr_ttl_reply_max '10'
	option old_port '53'
	option old_enabled '0'
	option old_auto_set_dnsmasq '0'

