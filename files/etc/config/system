
config system
	option hostname 'OpenWrt'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option compat_version '1.0'
	option timezone 'CST-8'
	option zonename 'Asia/Shanghai'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '1'
	list server 'ntp.aliyun.com'
	list server 'time1.cloud.tencent.com'
	list server 'time.ustc.edu.cn'
	list server 'cn.pool.ntp.org'

config led 'led_status'
	option name 'STATUS'
	option sysfs 'blue:status'
	option default '1'

config led 'led_wlan2g'
	option name 'WLAN2G'
	option sysfs 'blue:wlan2g'
	option trigger 'phy1tpt'

config led 'led_wlan5g'
	option name 'WLAN5G'
	option sysfs 'blue:wlan5g'
	option trigger 'phy0tpt'

config led 'led_wan'
	option name 'WAN(blue)'
	option sysfs 'blue:wan'
	option trigger 'switch0'
	option port_mask '0x20'

config led 'led_lan1'
	option name 'LAN1'
	option sysfs 'blue:lan1'
	option trigger 'switch0'
	option port_mask '0x02'

config led 'led_lan2'
	option name 'LAN2'
	option sysfs 'blue:lan2'
	option trigger 'switch0'
	option port_mask '0x04'

config led 'led_lan3'
	option name 'LAN3'
	option sysfs 'blue:lan3'
	option trigger 'switch0'
	option port_mask '0x08'

config led 'led_lan4'
	option name 'LAN4'
	option sysfs 'blue:lan4'
	option trigger 'switch0'
	option port_mask '0x10'

